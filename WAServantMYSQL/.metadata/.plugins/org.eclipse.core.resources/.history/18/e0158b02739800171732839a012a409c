        obj.put("age", new Integer(100));
        
        JSONArray list = new JSONArray();
        list.add("msg 1");
        list.add("msg 2");
        list.add("msg 3");
        
        obj.put("message", list);
        
        try{
        	FileWriter file = new FileWriter("/home/seppo/test.json");
        }catch (IOException e) {
        	e.printStackTrace();
        }
        System.out.print(obj);
    }
}
